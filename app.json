{
  "expo": {
    "name": "SDJ智慧倉儲系統",
    "version": "1.1.30",
    "android": {
      "package": "com.ibiza.sdj",
      "versionCode": 21,
      "permissions": [
        "CAMERA"
      ]
    },
    "platforms": [
      "ios",
      "android"
    ],
    "icon": "./assets/icon.png",
    "slug": "ibiza_sdj",
    "loading": {
      "hideExponentText": true,
      "backgroundImage": "./assets/splashscreen.png"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ibiza.sdj",
      "icon": "./assets/icon-ios.png"
    },
    "androidStatusBarColor": "#334393",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#334393",
    }
  }
}

