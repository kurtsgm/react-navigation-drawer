{
  "expo": {
    "name": "詮盈智慧倉儲系統",
    "version": "1.0.60",
    "android":{
      "package": "com.ibiza.chuanyin",
      "versionCode": 19,
      "permissions":["CAMERA"]
    },
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "icon": "./assets/icon.png",
    "slug": "ibiza_chuanyin",
    "loading": {
      "hideExponentText": true,
      "backgroundImage": "./assets/splashscreen.png"
    },
    "updates": {
      "enabled": true,  
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ibiza.chuanyin",
      "icon":"./assets/icon-ios.png"
    },
    "androidStatusBarColor": "#334393",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#334393",
      "supportsTablet": true
    }
  }
}